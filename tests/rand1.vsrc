import rand
import common

Event Input, Output {
  x: Int32
}

Event Dummy;

let t = new [Input, Output, Dummy];

-- we could use also rand.coin()
let U = rand.uniform(0, 1);

foreach n in common.range(0, $1) {
  let x = rand.uni(0, 100);
  if x in (0, 9) {
    --yield Input(pick from U) to t;
    yield Input(U.get()) to t;
    continue;
  }

  if x in (10, 19) {
    --yield Output(pick from U) to t;
    yield Output(U.get()) to t;
    continue;
  }

  yield Dummy to t;
}

