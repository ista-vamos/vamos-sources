import file
import regex
import string

let t = new [Input, Output, Write];
let t2 = new [Input, Output, Write];
let f = file.reader($1);

foreach line in f.lines() {
    let r = regex.search(line, "\s*(.)(.)\1(.)?");
    if r.matched() {
      let s = r.get(1);

      yield Input(s, string.concat(s, string.concat(s, s))) to t;

      if r.has(3) {
        yield Output(r.get(3)) to t2;
      }
    }
}

--out t to csv.writer("out.csv", "");
